Grammar

    0 $accept: program "End of file"

    1 $@1: %empty

    2 $@2: %empty

    3 program: body "End" $@1 $@2 subprograms

    4 body: declarations statements

    5 declarations: declarations type vars
    6             | declarations "Record" fields "Endrec" vars
    7             | declarations "Data" vals
    8             | %empty

    9 type: "Integer"
   10     | "Real"
   11     | "Logical"
   12     | "Character"

   13 vars: vars "," undef_variable
   14     | undef_variable

   15 undef_variable: "Id" "(" dims ")"
   16               | "Id"

   17 dims: dims "," dim
   18     | dim

   19 dim: "Integer constant"
   20    | "Id"

   21 fields: fields field
   22       | field

   23 field: type vars
   24      | "Record" fields "Endrec" vars

   25 vals: vals "," "Id" value_list
   26     | "Id" value_list

   27 value_list: "/" values "/"

   28 values: values "," value
   29       | value

   30 value: repeat "*" "+ or -" constant
   31      | repeat "*" constant
   32      | repeat "*" "String"
   33      | "+ or -" constant
   34      | constant
   35      | "String"

   36 repeat: "Integer constant"
   37       | %empty

   38 constant: "Integer constant"
   39         | "Real constant"
   40         | "Logical constant"
   41         | "String constant"

   42 statements: statements labeled_statement
   43           | labeled_statement

   44 labeled_statement: label statement
   45                  | statement

   46 label: "Integer constant"

   47 statement: simple_statement
   48          | compound_statement

   49 simple_statement: assignment
   50                 | goto_statement
   51                 | if_statement
   52                 | subroutine_call
   53                 | io_statement
   54                 | "Continue"
   55                 | "Return"
   56                 | "Stop"

   57 assignment: variable "=" expression
   58           | variable "=" "String"

   59 variable: variable ":" "Id"
   60         | variable "(" expressions ")"
   61         | "Id"

   62 expressions: expressions "," expression
   63            | expression

   64 expression: expression ".OR." expression
   65           | expression ".AND." expression
   66           | expression ".GT. or .GE. or .LT. or .LE. or .EQ. or .NE." expression
   67           | expression "+ or -" expression
   68           | expression "*" expression
   69           | expression "/" expression
   70           | expression "**" expression
   71           | ".NOT." expression
   72           | "+ or -" expression
   73           | variable
   74           | constant
   75           | "(" expression ")"

   76 goto_statement: "Goto" label
   77               | "Goto" "Id" "," "(" labels ")"

   78 labels: labels "," label
   79       | label

   80 if_statement: "If" "(" expression ")" label "," label "," label
   81             | "If" "(" expression ")" simple_statement

   82 subroutine_call: "Call" variable

   83 io_statement: "Read" read_list
   84             | "Write" write_list

   85 read_list: read_list "," read_item
   86          | read_item

   87 read_item: variable
   88          | "(" read_list "," "Id" "=" iter_space ")"

   89 iter_space: expression "," expression step

   90 step: "," expression
   91     | %empty

   92 write_list: write_list "," write_item
   93           | write_item

   94 write_item: expression
   95           | "(" write_list "," "Id" "=" iter_space ")"
   96           | "String"

   97 compound_statement: branch_statement
   98                   | loop_statement

   99 $@3: %empty

  100 branch_statement: "If" "(" expression ")" "Then" $@3 body tail

  101 $@4: %empty

  102 tail: "Else" $@4 body "Endif"
  103     | "Endif"

  104 $@5: %empty

  105 $@6: %empty

  106 loop_statement: "Do" $@5 "Id" $@6 "=" iter_space body "Enddo"

  107 $@7: %empty

  108 subprograms: subprograms $@7 subprogram
  109            | %empty

  110 subprogram: header body "End"
  111           | header body error

  112 header: type "Function" "Id" "(" formal_parameters ")"
  113       | "Subroutine" "Id" "(" formal_parameters ")"
  114       | "Subroutine" "Id"
  115       | error "Id"
  116       | "Subroutine" error
  117       | error error

  118 formal_parameters: type vars "," formal_parameters
  119                  | type vars
  120                  | error vars
  121                  | type error


